  "version": "1.0.0",
  "things-scene": true,
  "main": "src/index.js",
  "module": "dist/things-scene-<%= simpleModuleName %>.mjs",
  "jsnext:main": "dist/things-scene-<%= simpleModuleName %>.mjs",
  "author": "<%= username %>",
  "scripts": {
    "serve": "things-shell",
    "serve:dev": "things-shell-dev",
    "migration:run": "things-shell-migration",
    "migration:revert": "things-shell-migration --undo",
    "build:client": "webpack --config node_modules/@hatiolab/things-shell/webpack.config.js",
    "build": "rollup -c",
    "prepublish": "rollup -c"
  },
  "devDependencies": {
    "@hatiolab/things-shell": "*",
    "@things-scene/form": "*",
    "@things-scene/random": "*",
    "@things-scene/restful": "*",
    "@things-scene/table": "*"
  },
  "dependencies": {
    "@hatiolab/things-scene": "*"
  }
}
